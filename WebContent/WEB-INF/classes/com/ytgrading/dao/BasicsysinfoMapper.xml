<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ytgrading.dao.BasicsysinfoMapper">
	<resultMap type="HashMap" id="tableMsgMap">
		<result column="UA_INFO" property="key" />
		<association property="value" resultMap="java.lang.Object"></association>
	</resultMap>

	<select id="getUaMapByTableName" parameterType="java.lang.String"
		resultType="java.util.ArrayList">
		SELECT
			*
		FROM
			sys_tables_info
		WHERE
			1 = 1
		AND tablename = '${tableName}'
	</select>
</mapper>